import Phaser from 'https://codepen.io/kaka2023/pen/NWwjJWJ.js'
import Carrot from "https://codepen.io/kaka2023/pen/MWOmYwO.js"

export default class GoldenCarrot extends Carrot
{
  	/**
	 * @param {Phaser.Scene} scene 
	 * @param {number} x 
	 * @param {number} y 
	 * @param {string} texture 
	 */
	constructor(scene, x, y, texture = 'carrot')
	{
		super(scene, x, y, texture)
    this.points = 10;
    this.standtex = "GoldenBunnyStand"
    this.jumptex = "GoldenBunnyJump"
	}
}
  
  
